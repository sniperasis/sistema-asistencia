{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/panel",
      "dest": "/public/panel.html"
    },
    {
      "src": "/app",
      "dest": "/public/app-movil-funcional.html"
    },
    {
      "src": "/admin",
      "dest": "/public/panel.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
